@using Nop.Web.Models.Catalog;
@model CatalogPagingFilteringModel


@using (Html.BeginForm("SearchTuiJianProduct", "Catalog"))
 {
<b class="float-left tj-search-title ">商品分值：</b>
<b class="fPb-item float-left">
    @Html.TextBoxFor(m => m.MinScore,new { @class = "j_FPInput", name = "start_price", autofocus = "autofocus", autocomplete = "off", maxlength = "6" })
  
</b>
<i class="fPb-split">-</i>
<b class="fPb-item float-left">
    @Html.TextBoxFor(m => m.MaxScore, new { @class = "j_FPInput", name = "end_price", autofocus = "autofocus", autocomplete = "off", maxlength = "6" })
   
</b>
<b class="fPb-item float-left fPrice-btn-margin ">

    <button class="fPrice-btn" type="submit" >确定</button>
    @*<input class="fPrice-btn" type="submit" value="确定" />*@
    @*<a class="fPrice-btn" href="@Url.Action("SearchTuiJianProduct", "Catalog")" >确定</a>*@
    <button class="fPrice-empty" href="@Url.Action("SearchTuiJianProduct", "Catalog")">清空</button>
</b>
}